<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

    <changeSet id="1" author="liquibase">
        <createTable tableName="access_level">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="version" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="account" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_access_level_account" references="account(id)"/>
            </column>
            <column name="level" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_access_level_level" references="level(id)"/>
            </column>
            <column name="creation_date" type="timestamp" defaultValueComputed="CURRENT_TIMESTAMP">
                <constraints nullable="false"/>
            </column>
            <column name="created_by" type="BIGINT">
                <constraints foreignKeyName="fk_access_level_created_by" references="account(id)"/>
            </column>
            <column name="modification_date" type="date"/>
            <column name="modified_by" type="BIGINT">
                <constraints foreignKeyName="fk_access_level_modified_by" references="account(id)"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
